# focus window
meh - h : chunkc tiling::window --focus west
meh - j : chunkc tiling::window --focus south
meh - k : chunkc tiling::window --focus north
meh - l : chunkc tiling::window --focus east

# warp window
hyper - h : chunkc tiling::window --warp west
hyper - j : chunkc tiling::window --warp south
hyper - k : chunkc tiling::window --warp north
hyper - l : chunkc tiling::window --warp east

# rotate tree
meh - r : chunkc tiling::desktop --rotate 90

# toggle window fullscreen
meh - f : chunkc tiling::window --toggle fullscreen

# send window to monitor
meh - n : send-to-monitor.sh next
meh - p : send-to-monitor.sh prev

# toggle split
meh - t : chunkc tiling::window --toggle split

# equalize windows
meh - e : chunkc tiling::desktop --equalize