# focus window
cmd + ctrl + alt - h : chunkc tiling::window --focus west
cmd + ctrl + alt - j : chunkc tiling::window --focus south
cmd + ctrl + alt - k : chunkc tiling::window --focus north
cmd + ctrl + alt - l : chunkc tiling::window --focus east

# swap window
cmd + ctrl + shift + alt - h : chunkc tiling::window --swap west
cmd + ctrl + shift + alt - j : chunkc tiling::window --swap south
cmd + ctrl + shift + alt - k : chunkc tiling::window --swap north
cmd + ctrl + shift + alt - l : chunkc tiling::window --swap east

# rotate tree
cmd + ctrl + alt - r : chunkc tiling::desktop --rotate 90

# toggle window fullscreen
cmd + ctrl + alt - f : chunkc tiling::window --toggle fullscreen

# send window to monitor
cmd + ctrl + alt - n : send-to-monitor.sh next
cmd + ctrl + alt - p : send-to-monitor.sh prev

# close window
cmd + ctrl + alt - w : chunkc tiling::window --close