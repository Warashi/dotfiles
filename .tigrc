bind main ! ?git revert %(commit)
bind main <Ctrl-R> !git rebase -i %(commit)
bind main C ?git checkout %(commit)
bind main <Ctrl-C> ?git cherry-pick %(commit)
bind diff C ?git checkout %(commit) %(file)
bind status Y ?git stash push %(file)
bind refs C ?git checkout %(branch)%(tag)
bind main <Ctrl-O> ?@sh -c 'muscat open "$(gh repo view --json url --template "{{ .url }}")/commit/%(commit)"'
bind diff <Ctrl-O> ?@sh -c 'muscat open "$(gh repo view --json url --template "{{ .url }}")/blob/%(commit)/%(file)"'
bind status <Ctrl-O> ?@sh -c 'muscat open "$(gh repo view --json url --template "{{ .url }}")/blob/%(commit)/%(file)"'
