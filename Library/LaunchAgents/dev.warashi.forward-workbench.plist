<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>dev.warashi.forward-workbench</string>
    <key>ProgramArguments</key>
    <array>
      <string>ssh</string>
      <string>-N</string>
      <string>forward-workbench</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
      <key>NetworkState</key>
      <true/>
    </dict>
    <key>ThrottleInterval</key>
    <integer>60</integer>
  </dict>
</plist>
